{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-config-airbnb"],
  "jsRules": {},
  "rules": {
    "interface-name": false,
    "ter-arrow-parens": [false, "as-needed"],
    "no-console": [true, "log"],
    "array-type": false,
    "ban-types": false,
    "object-literal-sort-keys": false,
    "max-line-length": [true, 140]
  },
  "rulesDirectory": []
}
